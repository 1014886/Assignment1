<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
	http://www.springframework.org/schema/context 
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />

	<bean id="moviebean1"
		class="com.yash.spring_core_assignment_.Q13.MovieDetails">
		<property name="movieName" value="Pushpa"></property>
		<property name="ShowTime" value="9-12"></property>
		<property name="ticketPrice" value="180"></property>
	</bean>

	<bean id="moviebean2"
		class="com.yash.spring_core_assignment_.Q13.MovieDetails">
		<property name="movieName" value="RRR"></property>
		<property name="ShowTime" value="9-12"></property>
		<property name="ticketPrice" value="250"></property>
	</bean>

	<bean id="moviebean3"
		class="com.yash.spring_core_assignment_.Q13.MovieDetails">
		<property name="movieName" value="Shersaha"></property>
		<property name="ShowTime" value="7-10"></property>
		<property name="ticketPrice" value="150"></property>
	</bean>


</beans>