<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
	http://www.springframework.org/schema/context 
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	

	<bean id="custbean" class="com.yash.spring_core_assignment_.Q15.Customer">
		<property name="cid" value="1"></property>
		<property name="cname" value="swapnil"></property>
		<property name="cphone" value="8229354410"></property>
		<property name="caddress" value="pune"></property>
		<property name="itemObj" ref="itembean"></property>
	</bean>
	<bean id="itembean" class="com.yash.spring_core_assignment_.Q15.Item">
		<property name="itemid" value="10"></property>
		<property name="itemname" value="mobile"></property>
		<property name="itemprice" value="20000"></property>
		<property name="catobj" ref="catbean"></property>

	</bean>
	<bean id="catbean" class="com.yash.spring_core_assignment_.Q15.Category">
		<property name="catid" value="1000"></property>
		<property name="Catname" value="electronics"></property>
	</bean>

</beans>